{
  "name": "vite-plugin-import-graph",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.9.0",
  "description": "A Vite plugin that records your project's module import graph in a JSON file.",
  "author": "Justineo <justice360@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Justineo/vite-plugin-import-graph#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Justineo/vite-plugin-import-graph.git"
  },
  "bugs": "https://github.com/Justineo/vite-plugin-import-graph/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.7.5",
    "bumpp": "^9.7.1",
    "eslint": "^9.12.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.12.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
